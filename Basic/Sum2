public class Sum2 {
	public static void main(String[] args) {
		int num=4325,last=num%10 ,sum=0;
		int newn = num;
		int length = 0;
		while(num!=0) {
			length += 1;
			num=num/10;
		}
		double first = newn / Math.pow(10,length-1);
		int ff = (int) first;
		sum=last+ff;
		System.out.println(sum);

	}


}
